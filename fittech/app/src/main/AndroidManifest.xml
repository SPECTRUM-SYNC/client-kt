<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fittech"
        tools:targetApi="31">
        <activity
            android:name=".TelaInicialTres"
            android:exported="false"
            android:label="@string/title_activity_tela_inicial_tres"
            android:theme="@style/Theme.Fittech" />
        <activity
            android:name=".TelaInicialDois"
            android:exported="false"
            android:label="@string/title_activity_tela_inicial_dois"
            android:theme="@style/Theme.Fittech" /> <!-- Configuração do TelaInicialDois -->
        <activity
            android:name=".Questionario"
            android:exported="false"
            android:label="Questionario"
            android:theme="@style/Theme.Fittech" /> <!-- Configuração do Questionario -->
        <activity
            android:name=".TelaPerfil"
            android:exported="true"
            android:label="TelaPerfil"
            android:theme="@style/Theme.Fittech" /> <!-- Configuração da TelaCadastro -->
        <activity
            android:name=".TelaCadastro"
            android:exported="true"
            android:label="@string/title_activity_tela_cadastro"
            android:theme="@style/Theme.Fittech" /> <!-- Configuração da Home Activity -->
        <activity
            android:name=".Home"
            android:exported="true"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.Fittech" /> <!-- Configuração da TelaLogin -->
        <activity
            android:name=".TelaLogin"
            android:exported="true"
            android:label="@string/title_activity_tela_login"
            android:theme="@style/Theme.Fittech" /> <!-- Configuração da MainActivity -->
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Fittech">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>